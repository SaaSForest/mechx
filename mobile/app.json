{
  "expo": {
    "name": "mechX",
    "slug": "mechx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF5500"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mechx.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF5500"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mechx.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF7A00"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "068855c4-d1f8-4ea2-98e9-b9818c83edc7"
      }
    }
  }
}
